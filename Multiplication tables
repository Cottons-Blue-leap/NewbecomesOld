# multiplication tables

n = int(input())

for i in range(1, n+1):
    for j in range(1, n+1):
        a = i*j
        if j == 1:
            print(f'"{i} {j}ì€ {a}"', end = ' / ')
        elif j >= 6 and j <= 8:
            print(f'"{i} {j}ì— {a}"', end = ' / ')
        else:
            print(f'"{i} {j} {a}"', end = ' / ')
    print('\n')1
